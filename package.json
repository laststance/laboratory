{
  "name": "utility-collective",
  "license": "MIT",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "tsc -p react/tsconfig.json && vite build",
    "vite:typecheck": "tsc -p react/tsconfig.json",
    "vite:serve": "vite preview",
    "utility:build": "tsc --noEmit -p utility/tsconfig.json",
    "utility:typecheck": "tsc --noEmit -p utility/tsconfig.json",
    "typecheck": "yarn utility:typecheck && yarn vite:typecheck",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.1.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vite": "^2.5.8"
  }
}
